<h1>Edit Discussion</h1>

<%= @discussion.errors.full_messages.join(", ") %>
<%= form_for @discussion do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>
  <%= f.submit %>
<% end %>
