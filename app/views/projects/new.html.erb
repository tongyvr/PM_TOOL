<%= @project.errors.full_messages.join(", ") %>
<%= form_for @project do |f| %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_field :description%>
  </div>
  <div>
    <%= f.label :due_date %>
    <%= f.date_field :due_date%>
  </div>
  <%= f.submit %>
<% end %>
